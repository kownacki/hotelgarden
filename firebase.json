{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/@(villa-garden|olesnica-i-okolice|opinie|pokoje|konferencje|sale|kuchnia|restauracja|grill-garden|catering|wesela|chrzciny|komunie|bale-szkolne|galeria|wydarzenia|kontakt)",
        "destination": "/index.html"
      },
      {
        "source": "/wydarzenia/*",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
