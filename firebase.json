{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/@(villa-garden|atrakcje-okolicy|opinie|pokoje|konferencje|sale-konferencyjne|kuchnia|restauracja|grill-garden|catering|wesela|chrzciny|komunie|sale-bankietowe|galeria|wydarzenia|kontakt)",
        "destination": "/index.html"
      },
      {
        "source": "/wydarzenia/*",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/:url*/",
        "destination": "/:url",
        "type": 301
      },
      {
        "source": "/restaurant",
        "destination": "/restauracja",
        "type": 301
      },
      {
        "source": "/biznes",
        "destination": "/konferencje",
        "type": 301
      },
      {
        "source": "/przyjecia-rodzinne",
        "destination": "/wesela",
        "type": 301
      },
      {
        "source": "/relaks",
        "destination": "/atrakcje-okolicy",
        "type": 301
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
