{
  "name": "hotelgarden",
  "scripts": {
    "build": "rollup --config",
    "dev": "es-dev-server --app-index index.html --node-resolve",
    "deploy": "npm run deploy-hosting && npm run deploy-functions",
    "deploy-hosting": "firebase deploy --only hosting",
    "deploy-functions": "firebase deploy --only functions",
    "generate-index": "node utils/generateIndex.js",
    "bundle-lodash": "rollup --config rollup.lodashBuildConfig.js",
    "firebase-login": "firebase login"
  },
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "es-dev-server": "^1.60.0",
    "firebase": "^8.1.1",
    "firebase-tools": "^8.16.2",
    "lodash": "^4.17.21",
    "rollup": "^2.69.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-inline": "git+https://github.com/kownacki/ckeditor5-build-inline.git#stable",
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-formfield": "^0.25.3",
    "@material/mwc-snackbar": "^0.25.3",
    "@material/mwc-switch": "^0.25.3",
    "@material/mwc-textfield": "^0.25.3",
    "@polymer/app-layout": "^3.1.0",
    "@polymer/app-route": "^3.0.2",
    "@polymer/iron-ajax": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-image": "^3.0.2",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-fab": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.2",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "lit": "^2.1.3",
    "mk-firebase-utils": "git+https://github.com/kownacki/mk-firebase-utils.git#master",
    "mk-frontend-web-utils": "git+https://github.com/kownacki/mk-frontend-web-utils.git#master",
    "mkwc": "git+https://github.com/kownacki/mkwc.git#master",
    "pdfmake": "git+https://github.com/kownacki/pdfmake.git#hotelgarden"
  }
}
